@tailwind base;

p {
  @apply font-sans;
}
h1 {
  @apply text-2xl;
  @apply font-semibold;
  @apply font-sans;
}
h2 {
  @apply text-xl;
  @apply font-semibold;
  @apply font-sans;
}
h3 {
  @apply text-lg;
  @apply font-semibold;
  @apply font-sans;
}
h4 {
  @apply text-base;
  @apply font-semibold;
  @apply font-sans;
}
a {
  @apply text-blue-600;
}
@tailwind components;
@tailwind utilities;
