{
  "name": "okm-frontend-components",
  "version": "0.1.2",
  "dependencies": {},
  "scripts": {
    "start": "npm-run-all --parallel watch:css watch:component-css storybook",
    "build-dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build": "npm-run-all build:css build:react build:component-css build-dist",
    "build:react": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prettier": "npx prettier --write src/**/*.js",
    "build-storybook": "build-storybook -s public",
    "build:css": "postcss src/lib/css/tailwind.src.pcss -o src/lib/css/tailwind.css",
    "build:component-css": "postcss src/lib/components/**/*.pcss --base src --dir src --ext module.css",
    "watch:css": "postcss src/lib/css/tailwind.src.pcss -o src/lib/css/tailwind.css -w",
    "watch:component-css": "postcss src/lib/components/**/*.pcss --base src --dir src --ext module.css -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x",
    "@material-ui/core": "^4.x",
    "@material-ui/icons": "^4.x",
    "@material-ui/lab": "^4.x",
    "@material-ui/pickers": "^3.x",
    "@material-ui/styles": "^4.x",
    "prop-types": "^15.x",
    "tailwindcss": "^1.x",
    "lodash": "^4.x",
    "date-fns": "^2.x",
    "@date-io/date-fns": "^1.x",
    "ramda": "^0.26.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.2",
    "@babel/runtime": "^7.7.2",
    "@date-io/date-fns": "^1.3.12",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/lab": "^4.0.0-alpha.28",
    "@material-ui/pickers": "^3.2.6",
    "@material-ui/styles": "^4.5.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-centered": "^5.2.3",
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.6",
    "autoprefixer": "^9.7.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.2",
    "customize-cra": "^0.9.1",
    "date-fns": "^2.0.0-beta.5",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0",
    "tailwindcss": "^1.1.2"
  }
}
